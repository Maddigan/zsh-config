# -*- mode: shell-script -*-
# vim: set ft=zsh :

if [ -f ~/.zgen/zgen.zsh ]; then
    . ~/.zgen/zgen.zsh
    if ! zgen saved; then
        unset CLEARONSTART

        zgen load Tarrasch/zsh-functional
        zgen load vifon/deer
        zgen load vifon/zsh-image-extension

        zgen save
    fi

    autoload -U deer
    zle -N deer
    bindkey '\ek' deer
fi
